            return "7"
        elif (number == 8):
            return "8"
        elif  (number == 9):
            return "9"
        elif (number == 0):
            return "0"
        return

    def docSo(self, p ):
        strNum =''
        soLuong = len(str(p))
        soDao = 0

        number = int(p)
        if (number<0):
            strNum+="A" #số âm
            number = -number
            soLuong-=1

        while(number!=0):
            soDao = soDao * 10 + number % 10
            number = int(number / 10)
        if (soDao == 0):
            strNum+="0"
        while(soDao!=0):
            if ((soDao%10) == 1  and int(soDao%10)%10 == 0 and soLuong%3 == 2):
                strNum+="K" #mười
                soLuong-=1
                soDao = int(soDao / 10)
            else:
                if ((soDao % 10) == 1 and soLuong % 3 == 2):
                    strNum += "K" #mười
                    soLuong -= 1
                    soDao = int(soDao / 10)
                if (soLuong %3 ==0):
                    strNum += self.docChuSo(soDao % 10)
                    strNum+="T" #trăm
                if (soLuong % 3 == 2 and soDao%10 != 0):
                    strNum += self.docChuSo(soDao % 10)
                    strNum+= "I" #mươi
                elif (soLuong % 3 == 2 and soDao % 10 == 0 and int(soDao/10)%10 == 0):
                    strNum += ""
                elif(soLuong%3==2 and soDao%10==0):
                    strNum+="L" #lẻ
                if (soLuong % 3 == 1 and soDao%10 !=0):
                    strNum+=self.docChuSo(soDao%10)
            if (soLuong == 10):
                strNum += "B" #tỉ
            if (soLuong == 7):
                strNum += "M" #triệu
            if (soLuong == 4):
                strNum += "N" #ngàn
            soLuong=soLuong-1
            soDao = int(soDao/10)
        return strNum

    def docSoFull(self, p):
        number = float(p)
        nguyen = int(p)
        strNum = ''
        if (nguyen == number):
            strNum+=self.docSo(p)
            return strNum
        else:
            strNum+=self.docSo(str(nguyen))
            strNum+="P" #phẩy
            temp = number - nguyen
            if (temp<0):
                temp = -temp
            temp = round(temp,3)
            while (temp!=int(temp)):
                temp = temp*10
            strNum+=self.docSo(str(int(temp)))
        return strNum

    def onInput_input(self, p):
        if p=="error":
