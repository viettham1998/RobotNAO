    def priority(self, n):
        if n=="+" or n=="-":
            return 1
        if n=="*" or n=="/":
            return 2

    def push_stack(self, stack, item):
        stack.append(item)

    def pop_stack(self, stack):
        del stack[len(stack)-1]

    def stack_top(self, stack):
        if len(stack)==0:
            import time
            self.err()
            time.sleep(5)
            return "1"
        else:
            return stack[len(stack)-1]

    def infixToPostfix(self, infix):
        stack=[]
        postfix=''
        tmp=''
        for s in infix:
            if s=="(":
                self.push_stack(stack,s)
            if ord(s)>=48 and ord(s)<=57:
                tmp=tmp+s
            else:
                postfix=postfix+tmp+" "
                tmp=" "
            if s=="+" or s=="-" or s=="*" or s=="/":
                while(len(stack)>0 and self.priority(s)<=self.priority(self.stack_top(stack))):
                    postfix=postfix+self.stack_top(stack)+" "
                    self.pop_stack(stack)
                self.push_stack(stack,s)
            if s==")":
                while(self.stack_top(stack)<>"("):
                    postfix=postfix+self.stack_top(stack)+" "
                    self.pop_stack(stack)
                self.pop_stack(stack)
        if tmp<>"": postfix=postfix+tmp+" "
        while (len(stack)>0):
            postfix=postfix+self.stack_top(stack)+" "
            self.pop_stack(stack)
        return postfix

    def resultx(self, postfix):
        stack=[]
        tmp=''
        for s in postfix:
            if ord(s)>=48 and ord(s)<=57:
                tmp=tmp+s
            if s==" " and tmp<>" " and tmp<>"":
                self.push_stack(stack,tmp)
                tmp=""
            if s=="+":
                x=float(self.stack_top(stack))
                self.pop_stack(stack)
                y=float(self.stack_top(stack))
                self.pop_stack(stack)
                self.push_stack(stack,x+y)
            if s=="-":
                x=float(self.stack_top(stack))
                self.pop_stack(stack)
                y=float(self.stack_top(stack))
                self.pop_stack(stack)
                self.push_stack(stack,y-x)
            if s=="*":
                x=float(self.stack_top(stack))
                self.pop_stack(stack)
                y=float(self.stack_top(stack))
                self.pop_stack(stack)
                self.push_stack(stack,x*y)
            if s=="/":
                x=float(self.stack_top(stack))
                self.pop_stack(stack)
                y=float(self.stack_top(stack))
                self.pop_stack(stack)
                if x==0.0:
                    return "error"
                else:
                    self.push_stack(stack,y/x)
        return self.stack_top(stack)

    def onInput_input(self, p):
        self.infix=str(p)
        self.postfix=self.infixToPostfix(self.infix)
        res = self.resultx(self.postfix)
        if res<>"error":
            if int(res)==res: res=int(res)
        self.output(res)]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="input" type="3" type_size="1" nature="1" inner="0" tooltip="" id="2" /><Output name="output" type="0" type_size="1" nature="2" inner="0" tooltip="" id="3" /><Output name="err" type="1" type_size="1" nature="2" inner="0" tooltip="" id="4" /><Timeline enable="0"><BehaviorLayer name="behavior_layer1"><BehaviorKeyframe name="keyframe1" index="1"><Diagram><Box name="Diagram" id="10" localization="8" tooltip="This box is empty and should be used to create any box diagram you would like.&#x0A;&#x0A;To edit its diagram, double-click on it." x="987" y="652"><bitmap>media/images/box/box-diagram.png</bitmap><script language="4"><content><![CDATA[]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="4" /><Timeline enable="0"><BehaviorLayer name="behavior_layer1"><BehaviorKeyframe name="keyframe1" index="1"><Diagram /></BehaviorKeyframe></BehaviorLayer></Timeline></Box></Diagram></BehaviorKeyframe></BehaviorLayer></Timeline></Box><Box name="toString" id="12" localization="8" tooltip="Enter description here" x="586" y="643"><bitmap>media/images/box/box-diagram.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
